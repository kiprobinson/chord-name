{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": false,
    "noFallthroughCasesInSwitch": true
  },
  "lib": ["es2015"],
  "exclude": [
    "node_modules",
    "dist",
    "docs"
  ],
  "typedocOptions": {
    "excludePrivate": true
  }
}