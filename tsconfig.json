{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "module": "ES2022",
    "esModuleInterop": true,
    "target": "ES2022",
    "moduleResolution": "bundler",
    "declaration": true,
    "sourceMap": false,
    "noFallthroughCasesInSwitch": true
  },
  "lib": ["ES2022"],
  "exclude": [
    "node_modules",
    "dist",
    "docs"
  ],
  "typedocOptions": {
    "excludePrivate": true
  }
}