    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "chai": "^4.3.4",
    "eslint": "^8.7.0",
    "mocha": "^9.1.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
